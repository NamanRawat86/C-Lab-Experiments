#include <stdio.h>
#include <string.h>

struct Employee {
    char name[50];
    char address[100];
    float salary;
};

int main() {
    struct Employee emp[5], temp;
    int i, j;

    printf("Enter details for 5 employees (Name, Address, Salary):\n");
    for (i = 0; i < 5; i++) {
        printf("Employee %d:\n", i + 1);
        printf("Name: ");
        scanf("%s", emp[i].name);
        printf("Address: ");
        scanf("%s", emp[i].address);
        printf("Salary: ");
        scanf("%f", &emp[i].salary);
    }

    for (i = 0; i < 5 - 1; i++) {
        for (j = 0; j < 5 - i - 1; j++) {
            if (strcmp(emp[j].name, emp[j + 1].name) > 0) {
                temp = emp[j];
                emp[j] = emp[j + 1];
                emp[j + 1] = temp;
            }
        }
    }

    printf("\n--- Employees Sorted Alphabetically ---\n");
    for (i = 0; i < 5; i++) {
        printf("Name: %s, Address: %s, Salary: %.2f\n", emp[i].name, emp[i].address, emp[i].salary);
    }

    return 0;
}
